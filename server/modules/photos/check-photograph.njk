{% extends 'two-thirds-layout.njk' %}

{% block formContent %}

  <div class="govuk-form-group">
    <p>
      <img id="photograph" class="check-photo-img" src="{{ viewData.photoUrl }}" />
    </p>
  </div>

{% endblock %}

{# Overwrite this block to set the different button text #}
{% block continueButton %}
  {{ govukButton({
    text: "Use this photo"
  }) }}
{% endblock %}

{% block contentFooter %}
  <p class="govuk-body"><a href="add-photograph">Use a different photo</a></p>
{% endblock %}
